<footer id="login">

<%= form_for(@user, :as => user, :url => session_path(user)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :email %>
  <%= f.email_field :email %></div>
  <div><%= f.label :password %>
  <%= f.password_field :password %></div>

  <div><%= f.submit "Ever" %></div>
<% end %>

 </footer>

